{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "semi": true,
  "trailingComma": "none",
  "vueIndentScriptAndStyle": false,
  "importOrder": ["^vue", "<THIRD_PARTY_MODULES>", "^@/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
